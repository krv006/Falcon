                </button>
            {% endif %}

            {% for num in page_obj.paginator.page_range %}
                {% if num == 1 or num == page_obj.paginator.num_pages or num == page_obj.number %}
                    <a class="btn btn-sm btn-falcon-default {% if page_obj.number == num %}text-primary{% endif %} me-2"
                       href="?page={{ num }}">{{ num }}</a>
                {% elif num > page_obj.number|add:'-2' and num < page_obj.number|add:'2' %}
                    <a class="btn btn-sm btn-falcon-default me-2" href="?page={{ num }}">{{ num }}</a>
                {% elif num == page_obj.number|add:'-3' or num == page_obj.number|add:'3' %}
                    <span class="btn btn-sm btn-falcon-default me-2 fas fa-ellipsis-h"></span>
                {% endif %}
            {% endfor %}

